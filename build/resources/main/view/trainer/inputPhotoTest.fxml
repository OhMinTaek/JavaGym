<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.image.ImageView?>
<VBox alignment="CENTER" spacing="10" xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="controller.trainer.InputPhotoTestController"
            prefHeight="400.0" prefWidth="600.0">
    <Button text="이미지 선택" onAction="#selectPhoto" />
    <Button text="이미지 출력" onAction="#setPhoto" />
    <ImageView fx:id="imageView" fitHeight="300" fitWidth="300" pickOnBounds="true" preserveRatio="true" />

</VBox>
